{
  "name": "vim-js",
  "version": "0.0.1",
  "description": "A minimalist vimium",
  "scripts": {
    "install-deps": "npm install && npx playwright install && npx playwright install-deps",
    "gen-manifest": "node manifest/gen-manifest.js",
    "test": "playwright test",
    "test:headless": "xvfb-run playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0"
  }
}

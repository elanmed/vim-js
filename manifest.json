{
  "name": "vim-js",
  "description": "A minimalist vimium",
  "version": "0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"]
}

{
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "scripting", "storage"],
  "web_accessible_resources": [
    {
      "resources": ["content-keymaps.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
